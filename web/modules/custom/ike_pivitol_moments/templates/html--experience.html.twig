    {# Load Basic Scripts #}
    <script type="text/javascript" src="//use.typekit.net/vou7rxs.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    <script src="/modules/custom/ike_pivitol_moments/src/js/libs/require.js" data-main="/{{ variables.path }}/src/js/experience/main" type="text/javascript"></script>
    <script src="/modules/custom/ike_pivitol_moments/src/js/libs/jquery.js" type="text/javascript"></script>
    <script src="/modules/custom/ike_pivitol_moments/src/js/experience/extra.js" type="text/javascript"></script>
