    <div class="main-site">

      {% include '/modules/custom/ike_pivitol_moments/templates/header.html.twig' %}

      <header id="main-header">
        <ul class="theme-ul">
          <li class="option"><a class="link inactive" href="#/west_point">West Point</a></li>
