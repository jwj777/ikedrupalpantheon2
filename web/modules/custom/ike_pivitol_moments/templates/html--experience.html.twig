    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/{{ variables.path }}/src/img/appletouch/icon_114x114.png" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/{{ variables.path }}/src/img/appletouch/icon_144x144.png" />

    {# Load CSS #}
    <link rel="stylesheet" type="text/css" href="/{{ variables.path }}/src/css/video-js.min.css">
    <link rel="stylesheet" type="text/css" href="/{{ variables.path }}/src/css/experience_screen.css">
    <link rel="stylesheet" type="text/css" href="/{{ variables.path }}/src/css/screen.css">

