    <meta property="og:title" content="Pivotal Moments"/>
    <meta property="og:description" content="As the Supreme Commander of Allied Forces during World War II, Dwight D. Eisenhower would change the course of world history with his decision to launch the D-Day invasion of France. Go inside this extraordinary pivotal moment.">
    <meta property="og:image" content="/{{ variables.path }}/src/img/logos/EF_LOGO.png"/>
    <meta property="og:image:width" content="200"/>
    <meta property="og:image:height" content="200"/>

    {# Assign icon for apple touch devices #}
    <link rel="apple-touch-icon-precomposed" href="/{{ variables.path }}/src/img/appletouch/icon_57x57.png" />
